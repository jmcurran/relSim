citHeader("To cite relSim in publications use:")

bibentry(
  bibtype    = "manual",
  title    = 'relSim: A tool for simulating related DNA profiles and mixtures',
  author   = 'J. M. Curran',
  year     = '2022-08-29',
  note = 'R package version 0.3.5',
  url      = 'https://github.com/jmcurran/relSim',
  textVersion = paste("Curran, J. M. (2023) relSim: A tool for simulating related DNA profiles and mixtures. R package version 0.3.5."
  )
)
